{
  "name": "@funnelfox/billing",
  "version": "0.5.0-beta.1",
  "license": "MIT",
  "author": "Funnelfox",
  "description": "JavaScript SDK for Funnelfox billing with Primer integration",
  "main": "dist/funnelfox-billing.js",
  "module": "dist/funnelfox-billing.esm.js",
  "types": "src/types.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/types.d.ts",
      "import": "./dist/funnelfox-billing.esm.js",
      "require": "./dist/funnelfox-billing.cjs.js",
      "default": "./dist/funnelfox-billing.js"
    },
    "./types": {
      "types": "./src/types.d.ts"
    }
  },
  "files": [
    "dist/",
    "src/types.d.ts"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "build:watch": "rollup -c -w",
    "build:examples": "npm run build && npm pack && npm i ./funnelfox-billing-0.5.0-beta.1.tgz --prefix examples/basic && npm run build --prefix examples/basic",
    "dev": "rollup -c -w",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"*.{js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run lint && npm run format:check && npm run typecheck && npm run test",
    "prepublishOnly": "npm run validate && npm run build"
  },
  "keywords": [
    "payments",
    "billing",
    "checkout",
    "subscription",
    "primer",
    "sdk",
    "javascript",
    "funnelfox"
  ],
  "peerDependencies": {
    "@primer-io/checkout-web": "^2.x"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^30.0.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-import-css": "^4.1.2",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adaptyteam/funnelfox-billing-js.git"
  },
  "bugs": {
    "url": "https://github.com/adaptyteam/funnelfox-billing-js/issues"
  },
  "homepage": "https://github.com/adaptyteam/funnelfox-billing-js#readme"
}
